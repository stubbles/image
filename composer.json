{
    "name": "stubbles/image",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Simple image handling support.",
    "require": {
        "php": "^7.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "bovigo/assert": "^2.0",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\img\\": "src/main/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
